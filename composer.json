{
    "name": "laralum/laralum",
    "description": "The open source laravel administration panel",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.4",
        "ext-bcmath": "*",
        "doctrine/dbal": "2.*",
        "consoletvs/charts": "5.*",
        "consoletvs/identify": "1.*",
        "aitor24/localizer": "^1.4.2",
        "laralum/dashboard": "1.*",
        "laralum/settings": "1.*",
        "laralum/permissions": "1.*",
        "laralum/roles": "1.*",
        "laralum/users": "^1.0.5",
        "laralum/profile": "1.*",
        "laralum/notifications": "1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Campobadal",
            "email": "soc@erik.cat"
        },
        {
            "name": "Aitor Riba",
            "email": "contact@aitorriba.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laralum\\Laralum\\": "./src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Laralum\\Laralum\\LaralumServiceProvider"
            ]
        }
    }
}
